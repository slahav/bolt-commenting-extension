{
  "name": "cainc/commenting",
  "description": "Commenting Feature for Bolt",
  "type": "bolt-extension",
  "require": {
    "bolt/bolt": ">=2.0.0,<3.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Team Sulley",
      "email": "team-sulley@cainc.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "files": [
      "init.php"
    ],
    "psr-4": {
      "Bolt\\Extension\\Cainc\\Commenting\\": ""
    }
  },
  "autoload-dev": {
    "files": [
      "init.php"
    ]
  },
  "extra": {
    "bolt-assets": "assets"
  }
}
